<body class="text-center">
  <form class="form-signin" method="POST" action="{{ path('login') }}">
    <img class="mb-4" src="https://getbootstrap.com/docs/4.5/assets/img/favicons/apple-touch-icon.png" alt="" width="72" height="72">
    <small id="emailHelp" class="form-text text-muted">Informe o Email de Cadastro e senha para acessar.</small>
    <div class="form-group">
      <label for="email">Email </label>
      <input type="email" class="form-control" id="_email" name="_username" aria-describedby="emailHelp">
    </div>
    <div class="form-group">
      <label for="password">Senha</label>
      <input type="password" class="form-control" id="_password" name="_password">
    </div>

    <button type="submit" class="btn btn-primary btn-block">Acessar</button>
  </form>


</body>
