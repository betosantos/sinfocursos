            {{form_start(form, {'attr': {'class': ''}} )}}

            {% if app.user %}
            <div class="mb-3">
              Você já está logado como {{ app.user.username }}, <a href="{{ path('app_logout') }}">Sair</a>
            </div>
            {% endif %}

            {% for message in app.flashes('success') %}
              <div class="col-md-12">
                <div class="alert alert-success" role="alert">
                  {{ message }}
                </div>
              </div>
            {% endfor %}

            <h1 class="h3 mb-3 font-weight-normal">Cadastrar Usuário</h1>

            {{ form_row(form.nome, {'attr': {'class': 'form-control' }}) }}

            {{ form_row(form.email, {'attr': {'class': 'form-control' }}) }}

            {{ form_row(form.password, {'attr': {'class': 'form-control' }}) }}

            <br/>

            <button type="submit" class="btn btn-primary btn-lg btn-block">Cadastrar</button>

            {{form_end(form)}}

